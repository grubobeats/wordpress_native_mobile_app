<?xml version='1.0' encoding='utf-8'?>
<widget id="com.givemejobtoday.com.android_app" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>android_app</name>
    <description>
        A basic Framework7 template for PhoneGap.
    </description>
    <author email="vladan.paunovic.bg@gmail.com" href="https://givemejobtoday.com">
        Vladan Paunovic
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="SplashScreen" value="screen" />
    <preference name="SplashScreenDelay" value="3000" />
    <preference name="SplashMaintainAspectRatio" value="true" />
</widget>
